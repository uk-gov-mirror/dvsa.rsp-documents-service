{
  "name": "rsp-documents-service",
  "version": "4.0.0",
  "description": "",
  "repository": "<replace>",
  "author": "",
  "license": "MIT",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "start": "npm run copy-localfiles && npm run parallel",
    "presetup": "npm run copy-localfiles",
    "copy-localfiles": "npm run copy-sls && npm run copy-env && npm run copy-mockdata && npm run copy-models && npm run copy-openapi",
    "copy-sls": "./node_modules/.bin/copyfiles -f serverless/*.yml build",
    "copy-mockdata": "./node_modules/.bin/copyfiles mock-data/*.json build",
    "copy-models": "./node_modules/.bin/copyfiles -f src/models/*.json build/models",
    "copy-package": "./node_modules/.bin/copyfiles package.json build/",
    "copy-env": "./node_modules/.bin/copyfiles .env build/",
    "copy-openapi": "./node_modules/.bin/copyfiles openapiReplace.js build/",
    "deploy-AWS": "npm run copy-package && npm run copy-sls && cd ./build && npm install --production && sls deploy",
    "parallel": "./node_modules/.bin/run-p watch offline",
    "watch": "./node_modules/.bin/nodemon --watch src --exec npm run compile",
    "compile": "./node_modules/.bin/babel src -d build",
    "offline": "cd ./build && sls offline start --noAuth",
    "test": "./node_modules/.bin/mocha --require @babel/register --recursive \"src/**/*.unit.js\"",
    "test-i": "./node_modules/.bin/mocha --require @babel/register src/test/**/*.serviceInt.js",
    "lint": "./node_modules/.bin/eslint src",
    "lint:fix": "./node_modules/.bin/eslint src --fix",
    "build-openapi": "npm run compile && npm run copy-localfiles && cd build && sls openapi generate -f json && npm run openapireplace",
    "openapireplace": "node openapiReplace.js",
    "start-build": "rm -rf build && npm run copy-localfiles && npm run compile && npm run copy-package && npm run build-node-modules && npm run post-build-cleanup && npm run build-artifact && rm -f build/serverless.yml",
    "build-node-modules": "cd build && npm install --production",
    "build-artifact": "cd build && rm -rf artifacts && pwd && sls package --package artifacts",
    "post-build-cleanup": "rm -rf build/mock-data && rm -f build/package.json && rm -f build/openApiReplace.js && rm -f build/package-lock.json",
    "prepush": "npm run check-js-types && npm test && npm run lint && npm run check-security && npm run check-security-log",
    "check-security": "git secrets --scan",
    "check-security-log": "git log -p | scanrepo",
    "check-js-types": "tsc --noEmit -p .",
    "package": "webpack-cli --config webpack.production.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@babel/register": "^7.21.0",
    "@types/node": "^18.15.0",
    "archiver": "^5.3.1",
    "aws-sam-webpack-plugin": "^0.14.0",
    "babel-loader": "^9.1.2",
    "copyfiles": "^2.4.1",
    "eslint": "^8.35.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "expect": "^29.5.0",
    "fs-extra": "^11.1.0",
    "lodash": "^4.17.21",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.21",
    "npm-run-all": "^4.1.5",
    "replace": "^1.2.2",
    "rimraf": "^4.4.0",
    "serverless": "^3.28.1",
    "serverless-dotenv-plugin": "^4.0.2",
    "serverless-dynamodb-local": "^0.2.40",
    "serverless-offline": "^12.0.4",
    "serverless-openapi-documentation": "^0.4.0",
    "sinon": "^15.0.1",
    "supertest": "^6.3.3",
    "typescript": "^4.9.5",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.26.10",
    "@dvsa/rsp-validation": "^1.9.2",
    "aws-sdk": "^2.1332.0",
    "core-js": "^3.29.0",
    "install": "^0.13.0",
    "js-sha256": "^0.9.0",
    "serverless-dynamodb-client": "^0.0.2",
    "uuid": "^9.0.0"
  }
}
